programa {
  funcao inicio() {
    inteiro aaa
    escreva("Digite um numero:")
    leia(aaa)

    se(aaa < 0 )
    {
    escreva("O numero digitado eh negativo.")
    }
  
    senao se(aaa > 0)
    {
    escreva("O numero digitado eh positivo.")
    }
    
    senao
    {
    escreva("O valor digitado eh zero.")
    }
  }
}
