programa
{
    funcao inicio()
    {
        real num1, num2, resultado

        escreva("Digite o primeiro número: ")
        leia(num1)
        escreva("Digite o segundo número: ")
        leia(num2)

        resultado = (num1 - num2) / 10

        escreva("A décima parte da subtração é: ", resultado, "\n")
    }
}